(version 1)
(allow default)
(deny network*)
(deny file-write*)
(allow file-read* 
    (literal "/bin/ps")
    (literal "/dev/null")
    (literal "/dev/random")
    (literal "/dev/urandom")
    (literal "/dev/zero")
    (subpath "/private/var/db/dyld")
    (subpath "/usr/lib")
    (subpath "/usr/share")
    (subpath "/Library/Preferences/.GlobalPreferences.plist")
)
(allow process-exec (literal "/opt/homebrew/bin/z3"))